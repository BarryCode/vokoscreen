<!DOCTYPE node PUBLIC "-//freedesktop//DTD D-BUS Object Introspection 1.0//EN"
    "http://www.freedesktop.org/standards/dbus/1.0/introspect.dtd">
<node>
    <interface name="org.vokoscreen.gui">
        
       <method name="isVokoscreenLoaded">
          <arg name="text" type="s" direction="out"/>
       </method>

       <method name="showAllMethods">
          <arg name="text" type="s" direction="out"/>
       </method>
       
       <method name="startrecord">
          <arg name="text" type="s" direction="out"/>
       </method>

       <method name="stoprecord">
          <arg name="text" type="s" direction="out"/>
       </method>

       <method name="Tab">
          <arg name="text" type="s" direction="out"/>
          <arg name="value" type="s" direction="in"/>
       </method>

       <method name="FullScreen">
          <arg name="text" type="s" direction="out"/>
       </method>

       <method name="Window">
          <arg name="text" type="s" direction="out"/>
       </method>

       <method name="Area">
          <arg name="text" type="s" direction="out"/>
       </method>

       <method name="AreaReset">
          <arg name="text" type="s" direction="out"/>
       </method>

       <method name="MagnifierOn">
          <arg name="text" type="s" direction="out"/>
       </method>

       <method name="MagnifierOff">
          <arg name="text" type="s" direction="out"/>
       </method>
       
       <method name="CountDown">
          <arg name="text" type="s" direction="out"/>
          <arg name="value" type="s" direction="in"/>
       </method>

       <method name="AudioOn">
          <arg name="text" type="s" direction="out"/>
       </method>

       <method name="AudioOff">
          <arg name="text" type="s" direction="out"/>
       </method>

        <method name="CameraOn">
          <arg name="text" type="s" direction="out"/>
       </method>

       <method name="CameraOff">
          <arg name="text" type="s" direction="out"/>
       </method>

       <method name="CameraAllNames">
          <arg name="text" type="s" direction="out"/>
       </method>

       <method name="CameraCount">
          <arg name="text" type="s" direction="out"/>
       </method>

       <method name="CameraSetIndex">
          <arg name="text" type="s" direction="out"/>
          <arg name="value" type="s" direction="in"/>
       </method>

       <method name="quit">
       </method>

    </interface>
</node>
